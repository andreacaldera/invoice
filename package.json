{
  "name": "acal-invoice",
  "version": "0.30.0",
  "description": "invoice",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon src/server/index.js --exec babel-node",
    "data-import": "node script/import/index.js --exec babel-node",
    "data-export": "node script/export/index.js --exec babel-node",
    "lint": "eslint .",
    "test": "INVOICE_CONFIG=test echo 'no test yet...'",
    "test-watch": "INVOICE_CONFIG=test mocha --compilers js:babel-core/register test/** -w",
    "build": "NODE_ENV=production webpack -p && babel src/server -d dist/server && babel src/common -d dist/common",
    "watch": "NODE_ENV=development webpack -p --watch",
    "npm-check": "npm-check -u",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "prettier": "npm prettier "
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "test/*",
      "data/*",
      "style/*",
      "src/client/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreacaldera/invoice"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andreacaldera/invoice/issues"
  },
  "homepage": "http://acal-software.com/",
  "dependencies": {
    "accounting": "^0.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.13",
    "mongoose-hidden": "^1.4.2",
    "prop-types": "^15.5.6",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.0",
    "slang": "^0.3.0",
    "styled-components": "^3.2.3",
    "superagent": "^3.6.0",
    "url-pattern": "^1.0.3",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.0",
    "enzyme": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "husky": "^0.14.3",
    "jsx-loader": "^0.13.2",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-check": "^5.4.5",
    "prettier": "^1.14.3",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "^15.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6"
  }
}